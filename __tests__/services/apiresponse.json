            "localizations": []
        },
        {
            "id": 2,
            "documentId": "qjpbv7e0qpzskdi53zcx6dw4",
            "Location": "Auf dem Weg",
            "Content": "Ich weiß nicht, was ich denken soll. Ich weiß nicht, was ich sagen soll. Ich weiß nicht, was ich machen soll. Also gehe ich einfach.",
            "When": "2024-05-14",
            "Where": { "lat": 51.47121, "lng": 12.460033 },
            "createdAt": "2024-11-25T19:34:02.960Z",
            "updatedAt": "2024-11-25T19:34:02.960Z",
            "publishedAt": "2024-11-25T19:34:02.972Z",
            "locale": "de-DE",
            "localizations": []
        },
        {
            "id": 4,
            "documentId": "l916864sgvzkzztpwep8rkez",
            "Location": "Hörselberge",
            "Content": "Es regnet. Und so fühle ich mich auch. Alles nass. Ich will nicht mehr.",
            "When": "2024-05-21",
            "Where": { "lat": 50.95678, "lng": 10.453813 },
            "createdAt": "2024-11-26T21:20:30.779Z",
