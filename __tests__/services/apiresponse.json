        {
            "id": 8,
            "documentId": "s3pzxc6r1s7cfqwhl61h7pxd",
            "Location": "Tag 1",
            "Content": "Gemeinsam. Regen. Alle schlafen. Fast alle.",
            "When": "2024-05-30",
            "Where": { "lat": 50.817641, "lng": 8.611271 },
            "createdAt": "2024-11-27T21:45:58.597Z",
