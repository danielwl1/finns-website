        },
        {
            "id": 9,
            "documentId": "oykqdzlw74fskh5i1f9z6ixa",
            "Location": "Ein Zuhause",
            "Content": "Zum Platz der Sterne also laufe ich. Die Aussicht ist schön. Und nicht der Weg ist das Ziel, sondern die Leute auf ihm!",
            "When": "2024-06-07",
            "Where": { "lat": 50.730101, "lng": 5.993566 },
            "createdAt": "2024-11-27T21:47:33.671Z",
            "updatedAt": "2024-11-27T21:47:33.671Z",
            "publishedAt": "2024-11-27T21:47:33.668Z",
            "locale": "de-DE",
            "localizations": []
        },
        {
            "id": 10,
            "documentId": "dsfparm9u5iwidkog8fnufpk",
            "Location": "Am Wegesrand ",
            "Content": "Regen. Ankommen. Ruhe. Briefe. Einkaufen. Müde.",
